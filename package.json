{
  "name": "quark",
  "version": "0.1.0",
  "description": "Quark is an Electron base app that includes modern web technologies at its core.",
  "private": true,
  "main": "./main.js",
  "scripts": {
    "dev": "node build.js",
    "build": "rm -rf build/*; ./node_modules/.bin/webpack",
    "unit": "karma start tests/karma.conf.js --single-run",
    "test": "npm run unit",
    "start": "electron .",
    "distribute": "electron-packager ./ Unity --platform=darwin --arch=x64 --out ./dist --version 0.37.8 --overwrite --icon=./build/img/app-icon.icns"
  },
  "dependencies": {
    "bootstrap": "git+https://github.com/twbs/bootstrap.git#v4-dev",
    "bootstrap-datetimepicker": "git+https://github.com/Eonasdan/bootstrap-datetimepicker.git#v4.14.30",
    "font-awesome": "git+https://github.com/FortAwesome/Font-Awesome.git#v4.6.1",
    "jquery": "~2.2.0",
    "microplugin": "0.0.3",
    "moment": "~2.10.6",
    "open-sans": "git+https://github.com/bungeshea/open-sans.git#v1.1.0",
    "selectize": "git+https://github.com/brianreavis/selectize.js.git#v0.12.1",
    "sifter": "~0.4.3",
    "tether": "~1.1.1",
    "vue": "^1.0.24"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
